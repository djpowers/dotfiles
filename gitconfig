[user]
	name = Dave Powers
	email = djpowers89@gmail.com
[core]
	autocrlf = false
	safecrlf = true
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
	default = simple
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f

