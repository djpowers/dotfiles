  email = djpowers89@gmail.com
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[push]
  default = current
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  log-tree = log --graph --pretty=format:\"%C(auto)%h %d %s %Cblue<%an> %C(magenta)(%ar)\" --abbrev-commit
  clean-tags = !git tag -l | xargs git tag -d && git fetch --tags
